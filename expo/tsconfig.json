{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleSuffixes": [
      ".ios",
      ".android",
      ".native",
      ""
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "expo/.expo/types/**/*.ts",
    "expo-env.d.ts",
    "api/index.js",
    ".expo/types/**/*.ts",
    "nativewind-env.d.ts"
  ]
}